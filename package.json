{
  "name": "stepsyncai-health-apps",
  "version": "3.11.0",
  "description": "Health, wellness, and learning apps - Mental health support, medication tracker, and AWS learning",
  "main": "index.js",
  "scripts": {
    "mental": "node mental-health-tracker.js",
    "med": "node medication-tracker.js",
    "aws": "node aws-for-kids.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "*.js",
      "!jest.config.js",
      "!coverage/**",
      "!chart-utils.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 90,
        "lines": 81,
        "statements": 82
      }
    },
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "keywords": [
    "mental-health",
    "ptsd",
    "anxiety",
    "recovery",
    "workplace-accident",
    "medication",
    "tracker",
    "health",
    "wellness",
    "aws",
    "cloud",
    "learning",
    "education",
    "certification",
    "cloud-practitioner"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "dependencies": {
    "asciichart": "^1.5.25",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "node-cron": "^4.2.1",
    "node-notifier": "^10.0.1",
    "pdfkit": "^0.17.2"
  }
}
